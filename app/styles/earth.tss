	zIndex: 100,
	textAlign: 'center',
	backgroundColour: "transparent",
	font: {fontFamily: Alloy.CFG.font.ostrichMedium, fontSize: 30}
},
"#locationOptions":{
	top: 110,
	height: Ti.UI.SIZE,
	left: 5,
	right: 5,
	zIndex: 10
},
"#reminderSettings":{
	top: 180,
	left: 0,
	right: 0,
	height: Ti.UI.SIZE
},
"#nextPass":{
	top: 280,
	color: "#fff",
	font: {fontSize: 16, fontFamily: Alloy.CFG.font.ostrichBlack},
},
"#loggedInStatus":{
	backgroundColor: '#fff',
