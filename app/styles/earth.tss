"#earth":{
	backgroundImage: Alloy.CFG.backgroundImage	
},
"Label":{
	font: {fontFamily: Alloy.CFG.font.ostrichMedium, fontSize: 30},
	textAlign: 'center',
	color: "#fff"
},
".largeText": {
	font: {fontSize: 36, fontFamily: Alloy.CFG.font.ostrichBlack},
	width: Ti.UI.SIZE,
	textAlign: 'center',
	color: "#fff",
	shadowColor: '#000',
  	shadowOffset: {x:2, y:1}
},
".smallText": {
	font: {fontSize: 16, fontFamily: Alloy.CFG.font.ostrichBlack},
	width: Ti.UI.SIZE,
	color: "#fff",
	shadowColor: '#000',
  	shadowOffset: {x:2, y:1}
},
"#winTitle":{
	top: 10,
	height: Ti.UI.SIZE,
	width: Ti.UI.SIZE,
	zIndex: 10
},
"#cityName":{
